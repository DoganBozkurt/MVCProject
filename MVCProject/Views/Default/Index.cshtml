
@{
    Layout = null;
}

<!doctype html>
<html lang="tr">
@await Html.PartialAsync("Head")
<body data-bs-spy="scroll" data-bs-target="#navbar-example">
    <!--NAVBAR-->
    @await Html.PartialAsync("Navbar")
    <!-- BANNER -->
    @await Component.InvokeAsync("Banner")
    <!-- ABOUT -->
    @await Component.InvokeAsync("About")
    <!-- SERVİCES -->
    @await Component.InvokeAsync("Service")
    <!-- COUNTER -->
    @await Component.InvokeAsync("Counter")
    <!--TEAM-->
    @await Component.InvokeAsync("Team")
    <!--CONTACT-->
    @await Html.PartialAsync("SendMessageContact")
